<h1>Your possible pets</h1>
<% @pets.each do |pet| %>
	<div class="entry">
		<%= image_tag(pet.image_url) %>
		<h3><%= pet.name %> </h3>
		<h5>Species: <%= pet.species %></h5>
		<h5>Age: <%= pet.age %></h5>
		<%= button_to 'Pick me', consideration_items_path(pet_id: pet) %>
		<%= button_to 'Consider', consideration_items_path(pet_id: pet), remote: true %>
	</div>
<% end %>

